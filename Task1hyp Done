from statsmodels.stats.proportion import proportions_ztest

sales_control, applications_control, sales_test, applications_test = (10, 200, 15, 250) # входные данные

count = np.array([sales_control, sales_test])
nobs = np.array([applications_control, applications_test])

stat, pval = proportions_ztest(count, nobs)

alpha = 0.09
